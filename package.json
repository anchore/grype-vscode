{
	"name": "grype-vscode",
	"displayName": "Grype Vulnerability Scanner",
	"description": "Allows developers to quickly trigger a vulnerability scan of the project directory",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:grype-vscode.scan"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "grype-vscode.scan",
				"title": "Grype: Scan For Vulnerabilities"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
